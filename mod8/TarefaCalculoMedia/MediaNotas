package TarefaCalculoMedia;
/**
 * @author nnkidev
 */
public class MediaNotas
{
    public static void main(String[] args)
    {
        float nota1 = 6.5f;
        int   nota2 = 7;
        int   nota3 = 9;
        float nota4 = 4.5f;

        float media = (nota1 + nota2 + nota3 + nota4) / 4;

        System.out.println(
                "\nA média das quatro notas é de: " + media);
    }
}
